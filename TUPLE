#add item
tuple1=(4,6,2,8,3,1)
print(tuple1)
tuple2=(4,6,2,8,3,1,9)
print(tuple2)
tuple3=(4,6,2,8,3,15,20,25,4,6,2,8,3)
print(tuple3)
tuple4=(4,6,2,8,3,15,20,25,4,6,2,8,3,30)
print(tuple4)

#Length of a tuple
a=("cyfuno")
print(tuple(a))
print(len(a))

#Reverse tuple
a=("cyfuno")
print(tuple(a[: :-1])

#Repeated element in a tuple
a=(2,4,5,6,2,3,4,4,7)
n=int(input())
print(a)
p=a.count(n)
print(p)


